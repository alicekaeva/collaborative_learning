<form method="post" action="{{ path('app_material_delete', {'id': material.id}) }}" onsubmit="return confirm('Вы уверены, что хотите удалить этот материал?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ material.id) }}">
    <button><i class="fa-solid fa-trash-can"></i></button>
</form>
