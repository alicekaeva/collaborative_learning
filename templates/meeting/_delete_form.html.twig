<form method="post" action="{{ path('app_meeting_delete', {'id': meeting.id}) }}" onsubmit="return confirm('Вы уверены, что хотите удалить это собрание?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ meeting.id) }}">
    <button><i class="fa-solid fa-trash-can"></i></button>
</form>
